{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.22/MicrosoftTeams.schema.json",
  "manifestVersion": "1.22",
  "version": "1.0.7",
  "id": "${{TEAMS_APP_ID}}",
  "developer": {
    "name": "M365 Developer Advocates",
    "websiteUrl": "https://aka.ms/copilotdevcamp",
    "privacyUrl": "https://aka.ms/copilotdevcamp",
    "termsOfUseUrl": "https://aka.ms/copilotdevcamp"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "${{APP_DISPLAY_NAME}}",
    "full": "${{APP_DISPLAY_NAME}}: Agent for demonstrating multi-agent capabilities using the M365ChatAPI."
  },
  "description": {
    "short": "${{APP_DISPLAY_NAME}}",
    "full": "${{APP_DISPLAY_NAME}}: Agent for demonstrating multi-agent capabilities using the M365ChatAPI."
  },
  "accentColor": "#FFFFFF",
  "bots": [
    {
      "botId": "${{BOT_ID}}",
      "scopes": [ "copilot", "personal", "team", "groupChat" ],
      "supportsFiles": true,
      "isNotificationOnly": false,
      "commandLists": [
        {
          "scopes": [ "copilot", "personal", "team", "groupChat" ],
          "commands": [
            {
              "title": "Email",
              "description": "Send an email using the M365ChatAPI."
            },
            {
              "title": "Calendar",
              "description": "What meetings do I have tomorrow?"
            },
            {
              "title": "Files",
              "description": "Find the latest project proposal document"
            },
            {
              "title": "People",
              "description": "Who has expertise in Azure in my team?"
            },
            {
              "title": "General",
              "description": "Explain what microservices are"
            },
            {
              "title": "Multi-Intent",
              "description": "Summarize my emails and explain REST APIs"
            }
          ]
        }
      ]
    }
  ],
  "copilotAgents": {
    "customEngineAgents": [
      {
        "id": "${{BOT_ID}}",
        "type": "bot"
      }
    ]
  },
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "${{BOT_DOMAIN}}",
    "token.botframework.com"
  ],
  "webApplicationInfo": {
    "id": "${{BOT_ID}}",
    "resource": "api://botid-${{BOT_ID}}"
  }
}